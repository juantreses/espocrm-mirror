{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "eventType": {
            "type": "enum",
            "required": true,
            "options": [
                "called",
                "call_again",
                "wrong_number",
                "no_answer",
                "not_interested",
                "message_to_be_sent",
                "message_sent",
                "invited",
                "appointment_booked",
                "appointment_rescheduled",
                "appointment_cancelled",
                "attended",
                "no_show",
                "became_client",
                "not_converted",
                "still_thinking",
                "became_coach"
            ],
            "style": {
                "called": null,
                "call_again": null,
                "wrong_number": null,
                "no_answer": null,
                "not_interested": null,
                "message_to_be_sent": null,
                "message_sent": null,
                "invited": null,
                "appointment_booked": null,
                "appointment_rescheduled": null,
                "appointment_cancelled": null,
                "attended": null,
                "no_show": null,
                "became_client": null,
                "not_converted": null,
                "still_thinking": null,
                "became_coach": null
            },
            "default": "called",
            "maxLength": 100,
            "isCustom": true,
            "audited": true
        },
        "eventDate": {
            "notNull": false,
            "type": "datetime",
            "required": true,
            "default": "javascript: return this.dateTime.getNow(1);",
            "minuteStep": 30,
            "isCustom": true
        },
        "lead": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "lead": {
            "type": "belongsTo",
            "foreign": "cLeadEvents",
            "entity": "Lead",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}